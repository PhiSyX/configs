  "tokenColors": [
    /*
     * Comments
     */
    {
      "name": "comment",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8f8f8f"
      }
    },
    /** triple slash */
    {
      "name": "comment.triple-slash",
      "scope": [
        "comment.line.triple-slash",
        "comment.block.documentation storage.type",
        "comment.line.documentation",
        "comment.line.triple-slash keyword.operator.assignment"
      ],
      "settings": {
        "foreground": "#50A14F"
      }
    },
    /*** attribute */
    {
      "name": "comment.triple-slash.attribute",
      "scope": [
        "comment.line.triple-slash entity.other.attribute-name",
        "comment.line.triple-slash punctuation",
        "comment.line.triple-slash keyword.operator"
      ],
      "settings": {
        "foreground": "#74b4b8"
      }
    },
    /*** string */
    {
      "name": "comment.triple-slash.string",
      "scope": ["comment.line.triple-slash string.quoted"],
      "settings": {
        "foreground": "#98C379"
      }
    },
    /*** @doc */
    {
      "scope": [
        "comment.block.documentation storage.type.class",
        "comment.block.documentation.phpdoc",
        "keyword.other.phpdoc"
      ],
      "settings": {
        "foreground": "#212121"
      }
    },
    /*
     * Numeric
     */
    {
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#2993EB"
      }
    },
    {
      "scope": [
        "constant.numeric.hex",
        "constant.numeric entity.name.type.numeric",
        "keyword.other.unit"
      ],
      "settings": {
        "foreground": "#286491"
      }
    },
    /*
     * Keyword
     */
    /** wildcard */
    {
      "scope": "variable.language.wildcard",
      "settings": {
        "foreground": "#EF4836"
      }
    },
    /** logical */
    {
      "scope": "keyword.operator.logical",
      "settings": {
        "foreground": "#EF4836"
      }
    },
    /** assignment, increment */
    {
      "name": "+= | -= | *= | /= | %= | ^= | &= | |= | <<= | >>=",
      "scope": ["keyword.operator.assignment", "keyword.operator.increment"],
      "settings": {
        "foreground": "#EF4836"
      }
    },
    /** equal */
    {
      "name": "=",
      "scope": ["keyword.operator.assignment.equal"],
      "settings": {
        "foreground": "#949494"
      }
    },
    /** comparison */
    {
      "name": "== | != | <= | != | >=",
      "scope": ["keyword.operator.comparison"],
      "settings": {
        "foreground": "#949494"
      }
    },
    /** math */
    {
      "name": "+ | / | * | - | %",
      "scope": "keyword.operator.math",
      "settings": {
        "foreground": "#EF4836"
      }
    },
    {
      "scope": [
        "support.class.builtin",
        "support.constant.math",
        "support.constant.json",
        "source.php support.class.builtin punctuation.separator.inheritance"
      ],
      "settings": {
        "foreground": "#2993EB",
        "fontStyle": "underline"
      }
    },
    /** subpattern */
    {
      "name": "@",
      "scope": "keyword.operator.subpattern",
      "settings": {
        "foreground": "#2993EB"
      }
    },
    /** dot, key-value, range */
    {
      "name": ". | .. | k:v",
      "scope": [
        "keyword.operator.access.dot",
        "keyword.operator.key-value",
        "keyword.operator.range"
      ],
      "settings": {
        "foreground": "#2993EB"
      }
    },
    /** arrow */
    {
      "name": "=> | ->",
      "scope": [
        "keyword.operator.arrow.fat",
        "keyword.operator.key",
        "keyword.operator.arrow.skinny"
      ],
      "settings": {
        "foreground": "#2993EB"
      }
    },
    /** question */
    {
      "name": "?",
      "scope": ["keyword.operator.question", "keyword.operator.nullable-type"],
      "settings": {
        "foreground": "#D4803B"
      }
    },
    /** callback */
    {
      "name": "|name|",
      "scope": ["meta.function.call keyword.operator.logical"],
      "settings": {
        "foreground": "#46A0B0"
      }
    },
    /*
     * Ponctuation
     */
    {
      "name": "< | >",
      "scope": [
        "punctuation.brackets.angle",
        "punctuation.definition.typeparameters"
      ],
      "settings": {
        "foreground": "#ca2b1a"
      }
    },
    /*
     * Lifetime
     */
    {
      "name": "'a | &'a",
      "scope": ["punctuation.definition.lifetime", "entity.name.type.lifetime"],
      "settings": {
        "foreground": "#e46bda",
        "fontStyle": "italic"
      }
    },
    /*
     * Macro
     */
    {
      "name": "macro_name",
      "scope": ["entity.name.function.macro", "entity.name.type.macro"],
      "settings": {
        "foreground": "#ce9de4",
        "fontStyle": "underline"
      }
    },
    {
      "name": "macro_rules!",
      "scope": "entity.name.function.macro.rules",
      "settings": {
        "foreground": "#b252df"
      }
    },
    {
      "name": "$ | {i}",
      "scope": ["keyword.operator.macro.dollar", "meta.interpolation"],
      "settings": {
        "foreground": "#b252df"
      }
    },
    {
      "name": "$var_name",
      "scope": "variable.other.metavariable.name",
      "settings": {
        "foreground": "#d49bee"
      }
    },
    {
      "name": "type",
      "scope": "variable.other.metavariable.specifier",
      "settings": {
        "foreground": "#b252df",
        "fontStyle": "underline"
      }
    },
    /*
     * Namespaces
     */
    {
      "name": "path::namespace",
      "scope": "meta.use",
      "settings": {
        "foreground": "#b252df"
      }
    },
    {
      "name": "::",
      "scope": ["keyword.operator.namespace"],
      "settings": {
        "foreground": "#b252df"
      }
    },
    /*
     * String
     */
    {
      "name": "\" | ' | `",
      "scope": ["punctuation.definition.string"],
      "settings": {
        "foreground": "#212121"
      }
    },
    {
      "name": "b | r",
      "scope": "string.quoted.byte",
      "settings": {
        "foreground": "#2993EB"
      }
    },
    {
      "name": "strings",
      "scope": ["string.quoted", "string.quoted.single"],
      "settings": {
        "foreground": "#D4803B"
      }
    },
    {
      "name": "char",
      "scope": "string.quoted.single.char",
      "settings": {
        "foreground": "#2993EB"
      }
    },
    /** regex */
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#74b4b8"
      }
    },
    {
      "scope": ["string.regexp keyword.other", "string.regexp punctuation"],
      "settings": {
        "foreground": "#4D814D"
      }
    },
    /*
     * Attribute
     */
    {
      "scope": [
        "punctuation.definition.attribute",
        "punctuation.brackets.attribute",
        "punctuation.definition.annotation",
        "meta.attribute punctuation.brackets.round"
      ],
      "settings": {
        "foreground": "#DFC3EC"
      }
    },
    {
      "scope": [
        "meta.attribute",
        "meta.declaration.annotation storage.type.annotation"
      ],
      "settings": {
        "foreground": "#b252df"
      }
    },
    {
      "scope": ["meta.attribute entity.name.type"],
      "settings": {
        "foreground": "#212121",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["source.python meta.attribute"],
      "settings": {
        "foreground": "#212121"
      }
    },
    /*
     * CONSTANTE
     */
    {
      "scope": ["constant.character"],
      "settings": {
        "foreground": "#ca2b1a"
      }
    },
    {
      "scope": [
        "constant.language.true",
        "constant.language.false",
        "constant.language.boolean",
        "constant.language.bool",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language.c",
        "constant.language.json",
        "constant.language.go",
        "constant.language.php",
        "constant.other.caps"
      ],
      "settings": {
        "foreground": "#46A0B0"
      }
    },
    /*
		 * Storage
		{
			"scope": [
				"storage",
			],
			"settings": {
				"foreground": "#3fb0cc"
			}
		},
		/** type
		{
			"scope": "storage.type",
			"settings": {
				"foreground": "#EF4836",
				"fontStyle": "underline"
			}
		},
		/** const, let, var */
    {
      "scope": [
        "keyword.other storage.type",
        "meta.var.expr storage.type",
        "storage.modifier.specifier.const",
        "storage.modifier.const"
      ],
      "settings": {
        "foreground": "#EF4836",
        "fontStyle": ""
      }
    },
    /** mut */
    {
      "scope": "storage.modifier.mut",
      "settings": {
        "foreground": "#EF4836",
        "fontStyle": "underline"
      }
    },
    /** function */
    {
      "scope": [
        "keyword.other.fn",
        "keyword.function",
        "storage.type.function",
        "keyword.control.def"
      ],
      "settings": {
        "foreground": "#1f8aa5",
        "fontStyle": ""
      }
    },
    /*
     * Ponctuation
     */
    {
      "scope": ["punctuation", "meta.brace"],
      "settings": {
        "foreground": "#21212159"
      }
    },
    /** comma */
    {
      "scope": ["punctuation.separator.comma", "punctuation.comma"],
      "settings": {
        "foreground": "#2993EB"
      }
    },
    /*
     * Keywords
     */
    /** control */
    {
      "scope": "keyword.control",
      "settings": {
        "foreground": "#949494"
      }
    },
    /** other */
    {
      "scope": "keyword.other.rust",
      "settings": {
        "foreground": "#949494"
      }
    },
    /** async, using, extern, crate */
    {
      "scope": [
        "storage.modifier.async",
        "storage.modifier.await",
        "keyword.other.crate",
        "keyword.other.using"
      ],
      "settings": {
        "foreground": "#46A0B0"
      }
    },
    /** op√©rateurs */
    /*** borrow, (de)reference, pointer */
    {
      "scope": [
        "keyword.operator.address",
        "keyword.operator.borrow",
        "keyword.operator.dereference",
        "storage.modifier.pointer",
        "storage.modifier.reference",
        "punctuation.separator.pointer-access"
      ],
      "settings": {
        "foreground": "#ca2b1a"
      }
    },
    /*
     * Entity
     */
    /** type */
    /*** declaration */
    {
      "scope": [
        "entity.name.type.numeric",
        "entity.name.type.primitive",
        "meta.function.definition entity.name.type",
        "meta.arrow entity.name.type",
        "support.type.primitive",
        "storage.type.java",
        "storage.type.primitive.java",
        "storage.type.built-in",
        "storage.type.byte",
        "storage.type.numeric",
        "storage.type.string",
        "keyword.other.type.php"
      ],
      "settings": {
        "foreground": "#EF4836",
        "fontStyle": "underline"
      }
    },
    {
      "scope": ["keyword.control.as", "keyword.operator.expression"],
      "settings": {
        "foreground": "#EF4836"
      }
    },
    /*** builtin */
    {
      "scope": [
        "entity.name.type.option",
        "entity.name.type.result",
        "support.function.builtin"
      ],
      "settings": {
        "foreground": "#212121",
        "fontStyle": "bold underline"
      }
    },
    /** function */
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#212121",
        "fontStyle": "bold"
      }
    },
    /*
     * Meta
     */
    {
      "scope": [
        "meta.preprocessor keyword",
        "source.c keyword.control.directive",
        "source.cpp keyword.control.directive",
        "meta.use keyword.other",
        "meta.use punctuation.comma.rust"
      ],
      "settings": {
        "foreground": "#b252df"
