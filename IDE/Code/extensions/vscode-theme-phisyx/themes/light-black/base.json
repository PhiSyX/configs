    "tokenColors": [
        {
            "scope": [
                "support.function.builtin"
            ],
            "settings": {
                "foreground": "#212121",
                "fontStyle": "bold underline"
            }
        },
        {
            "scope": [
                "keyword.operator.comparison"
            ],
            "settings": {
                "foreground": "#cbc8c8"
            }
        },
        {
            "scope": [
                "comment",
                "comment.block"
            ],
            "settings": {
                "foreground": "#8f8f8f"
            }
        },
        {
            "scope": [
                "comment.line.documentation"
            ],
            "settings": {
                "foreground": "#8f8f8f",
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "keyword.operator.logical",
                "keyword.operator.assignment",
                "keyword.operator.arithmetic",
                "keyword.operator.increment",
				"keyword.operator.decrement"
            ],
            "settings": {
                "foreground": "#ef4836"
            }
        },
        {
            "scope": [
                "keyword.control.return"
            ],
            "settings": {
                "foreground": "#ef4836",
                "fontStyle": "underline"
            }
        },
        {
            "scope": [
                "string.quoted.double constant.character"
            ],
            "settings": {
                "foreground": "#ca2b1a"
            }
        },
        
		{
            "scope": [
                "string.quoted.double",
                "string.quoted.single"
            ],
            "settings": {
                "foreground": "#f98b8b"
            }
        },
		{
            "scope": [
                "string.quoted.double punctuation.definition",
                "string.quoted.single punctuation.definition"
            ],
            "settings": {
                "foreground": "#f78978",
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "constant.language"
            ],
            "settings": {
                "foreground": "#46a0b0"
            }
        },
        {
            "scope": [
                "constant.character.escape"
            ],
            "settings": {
                "foreground": "#3fb0cc"
            }
        },
        {
            "scope": [
                "keyword.operator.math",
                "keyword.operator.arrow.fat",
                "keyword.operator.arrow.skinny",
                "constant.numeric.decimal",
                "punctuation.comma"
            ],
            "settings": {
                "foreground": "#2993eb"
            }
        }
    ]
}